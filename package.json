{
  "name": "pick-coredump",
  "displayName": "Pick coredump",
  "description": "Command to find coredump file",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pick-coredump.search-coredump",
        "title": "Pick CoreDump path",
        "enablement": "false"
      }
    ],
    "configuration": {
      "title": "Pick coredump",
      "properties": {
        "pick-coredump.coredump-dir": {
          "title": "Coredumps directory",
          "type": "string",
          "description": "Directory where core dumps are generated"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTests.js",
    "lint": "eslint ./src",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "esbuild": "^0.27.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}